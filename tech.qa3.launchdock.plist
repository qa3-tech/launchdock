<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>tech.qa3.launchdock</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Applications/LaunchDock.app/Contents/MacOS/launchdock</string>
        <string>start</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/tmp/launchdock.stdout</string>

    <key>StandardErrorPath</key>
    <string>/tmp/launchdock.stderr</string>
</dict>
</plist>
